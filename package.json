{
  "name": "node-api",
  "version": "1.0.0",
  "description": "Fully Featured Node.js API with SQL DB",
  "author": "Mike Donahue <mtdonahue1@gmail.com>",
  "scripts": {
    "start:dev": "node src/app.js | bunyan",
    "start": "NODE_ENV=production node src/app.js | bunyan",
    "nodemon": "nodemon src/app.js | bunyan",
    "test": "NODE_ENV=test nyc mocha 'src/**/*.test.js'",
    "test:bb": "NODE_ENV=test mocha 'src/**/*.test.bb.js' --timeout 30000 --exit",
    "test:watch": "NODE_ENV=test nodemon --exec \"mocha 'src/**/*.test.js'\"",
    "lint": "eslint 'config/*.js' 'migrations/*.js' 'seeders/*.js' 'src/**/*.js'",
    "migrate": "knex migrate:latest",
    "migrate:undo": "knex migrate:rollback",
    "seed": "knex seed:run",
    "mysql:restart": "mysql.server restart"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "include": [
      "src/api/**",
      "src/middleware/**",
      "!src/models/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sidewalksalsa/node-api.git"
  },
  "dependencies": {
    "accesscontrol": "^1.5.3",
    "apollo-server-express": "^1.3.6",
    "app-module-path": "^2.2.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "bunyan-prettystream": "^0.1.3",
    "config": "^1.26.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "csurf": "^1.9.0",
    "date-fns": "^1.29.0",
    "express": "^4.15.3",
    "express-healthcheck": "^0.1.0",
    "graphql": "^0.13.2",
    "graphql-resolvers": "^0.2.2",
    "graphql-tools": "^3.0.1",
    "helmet": "^3.6.1",
    "jsonwebtoken": "^8.0.0",
    "knex": "^0.14.4",
    "lodash.difference": "^4.5.0",
    "md5": "^2.2.1",
    "morgan": "^1.8.2",
    "morgan-json": "^1.1.0",
    "mysql2": "^1.4.2",
    "object-path": "^0.11.4",
    "passport": "^0.4.0",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-local": "^1.0.0",
    "pretty-hrtime": "^1.0.3",
    "response-time": "^2.3.2",
    "shortid": "^2.2.8",
    "sparkpost": "^2.1.2",
    "stripe": "^5.5.0",
    "validate.js": "^0.12.0",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chance": "^1.0.11",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.12.0",
    "nyc": "^11.2.1",
    "proxyquire": "^2.0.1",
    "sinon": "^5.0.7",
    "supertest": "^3.1.0"
  },
  "private": true,
  "license": "MIT"
}
